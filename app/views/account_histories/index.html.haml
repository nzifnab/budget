.sidebar-container.box-content
  %h1
    - if account.present?
      .float-right{class: account.amount_class}= account.display_amount
    = account.try(:name) || "Transaction History"
  - if account_histories.present?
    %ul.tabs
      %li.tab-active= link_to "All", "#", class: "js-prevent-default"
      -#%li= link_to "Quick Funds", "#"
      -#%li= link_to "Income", "#"

    %ul.boxed-list
      = render account_histories, details_account_name: account.blank?

    = pagination(account_histories)
  .break
